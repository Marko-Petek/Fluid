{
   "version": "2.0.0",
   "tasks": [
      {
         "label": "Rebuild: Debug",
         "command": "dotnet",
         "type": "process",
         "args": ["msbuild", "-p:Configuration=Debug", "-t:Rebuild", "Fluid.sln"],
         "problemMatcher": "$msCompile",
         "group": {
            "kind": "build",
            "isDefault": true
         }
      },
      {
         "label": "Rebuild",
         "command": "dotnet",
         "type": "process",
         "args": ["msbuild", "/t:Rebuild", "Fluid.sln"],
         "problemMatcher": "$msCompile",
         "group": {
            "kind": "build",
            "isDefault": true
         }
      },
      {
         "label": "Build",
         "command": "dotnet",
         "type": "process",
         "args": [
            "build"
         ],
         "problemMatcher": "$msCompile",
         "group": {
            "kind": "build",
            "isDefault": true
         }
      },
      {
         "label": "Test",
         "command": "dotnet",
         "type": "process",
         "args": ["test", "${workspaceFolder}/Tests/Tests.csproj"],
         "options": {
            "cwd": "./"
         },
         "problemMatcher": "$msCompile",
         "group": {
            "kind": "test",
            "isDefault": true
         },
      },
      {
         "label": "Run ChannelFlow",
         "command": "dotnet",
         "type": "process",
         "args": [
            "run",
            "--project",
            "${workspaceFolder}/ChannelFlow/ChannelFlow.csproj"
         ],
         "options": {
            "cwd": "./ChannelFlow"
         },
         "problemMatcher": "$msCompile",
         "group": "none"
      },
      {
         "label": "Run CavityFlow",
         "command": "dotnet",
         "type": "process",
         "args": [
            "run",
            "--project",
            "${workspaceFolder}/Demos/CavityFlow/CavityFlow.csproj"
         ],
         "options": {
            "cwd": "./Demos/CavityFlow"
         },
         "problemMatcher": "$msCompile",
         "group": "none"
      },
      {
         "label": "Run NodeCreation",
         "command": "dotnet",
         "type": "process",
         "args": [
            "run",
            "--project",
            "${workspaceFolder}/Seminar/NodeCreation/NodeCreation.csproj"
         ],
         "options": {
            "cwd": "./Seminar/NodeCreation"
         },
         "problemMatcher": "$msCompile",
         "group": "none"
      }
   ]
}