{
"version": "2.0.0",
"tasks": [
{
   "label": "Rebuild, debug, x64, REPORT",
   "command": "dotnet",
   "type": "process",
   "args": ["msbuild", "-p:Configuration=Debug;Platform=x64;DefineConstants=REPORT", "-t:Rebuild", "Fluid.sln"],
   "problemMatcher": "$msCompile",
   "group": "build" },
{
      "label": "Rebuild, debug, x64, noDefines",
      "command": "dotnet",
      "type": "process",
      "args": ["msbuild", "-p:Configuration=Debug;Platform=x64;DefineConstants=", "-t:Rebuild", "Fluid.sln"],
      "problemMatcher": "$msCompile",
      "group": "build" },
{
   "label": "Rebuild, release, x64, REPORT",
   "command": "dotnet",
   "type": "process",
   "args": ["msbuild", "-p:Configuration=Release;Platform=x64;DefineConstants=REPORT", "/t:Rebuild", "Fluid.sln"],
   "problemMatcher": "$msCompile",
   "group": "build" },
{
   "label": "Build, noRestore, debug, x64, REPORT)",
   "command": "dotnet",
   "type": "process",
   "args": ["msbuild", "-p:Configuration=Debug;Platform=x64;DefineConstants=REPORT", "-restore:False", "Fluid.sln"],
   "problemMatcher": "$msCompile",
   "group": {
      "kind": "build",
      "isDefault": true } },
{
   "label": "ChannelFlow (Run)",
   "command": "dotnet",
   "type": "process",
   "args": ["run", "--project", "${workspaceFolder}/ChannelFlow/ChannelFlow.csproj" ],
   "options": {
      "cwd": "./ChannelFlow" },
   "problemMatcher": "$msCompile",
   "group": "none" },
{
   "label": "CavityFlow (Debug)",
   "command": "dotnet",
   "type": "process",
   "args": [
      "run",
      "-p:Configuration=Debug",
      "--no-build",
      "--project",
      "${workspaceFolder}/Demos/CavityFlow/CavityFlow.csproj" ],
   "options": {
      "cwd": "./Demos/CavityFlow" },
   "problemMatcher": "$msCompile",
   "group": "none"
},
{
   "label": "Run NodeCreation",
   "command": "dotnet",
   "type": "process",
   "args": [
      "run",
      "--project",
      "${workspaceFolder}/Seminar/NodeCreation/NodeCreation.csproj" ],
   "options": {
      "cwd": "./Seminar/NodeCreation" },
   "problemMatcher": "$msCompile",
   "group": "none"
},
{
   "label": "Run Tests",
   "command": "dotnet",
   "type": "process",
   "args": [
      "run", "--no-build", "-p", "${workspaceFolder}/Main/Main.csproj", "Tests" ],
   "options": {
      "cwd": "${workspaceFolder}" },
   "problemMatcher": "$msCompile",
   "group": "none"
}
]
}