{
"version": "2.0.0",
"tasks": [
{
   "label": "Rebuild (Debug, REPORT)",
   "command": "dotnet",
   "type": "process",
   "args": ["msbuild", "-p:Configuration=Debug", "-p:DefineConstants=REPORT", "-t:Rebuild", "Fluid.sln"],
   "problemMatcher": "$msCompile",
   "group": "build" },
{
   "label": "Rebuild (Release, REPORT)",
   "command": "dotnet",
   "type": "process",
   "args": ["msbuild", "-p:Configuration=Release", "-p:DefineConstants=REPORT", "/t:Rebuild", "Fluid.sln"],
   "problemMatcher": "$msCompile",
   "group": "build" },
{
   "label": "Build",
   "command": "dotnet",
   "type": "process",
   "args": ["build"],
   "problemMatcher": "$msCompile",
   "group": {
      "kind": "build",
      "isDefault": true } },
{
   "label": "Test",
   "command": "dotnet",
   "type": "process",
   "args": ["run", "--project", "${workspaceFolder}/Main/Main.csproj", "--launch-profile", "LaunchTests"],
   "options": {
      "cwd": "./" },
   "problemMatcher": "$msCompile",
   "group": {
      "kind": "test",
      "isDefault": true } },
{
   "label": "ChannelFlow (Run)",
   "command": "dotnet",
   "type": "process",
   "args": [
      "run",
      "--project",
      "${workspaceFolder}/ChannelFlow/ChannelFlow.csproj" ],
   "options": {
      "cwd": "./ChannelFlow" },
   "problemMatcher": "$msCompile",
   "group": "none" },
{
   "label": "CavityFlow (Debug)",
   "command": "dotnet",
   "type": "process",
   "args": [
      "run",
      "-c Debug",
      "--no-build",
      "--project",
      "${workspaceFolder}/Demos/CavityFlow/CavityFlow.csproj" ],
   "options": {
      "cwd": "./Demos/CavityFlow"
   },
   "problemMatcher": "$msCompile",
   "group": "none"
},
{
   "label": "Run NodeCreation",
   "command": "dotnet",
   "type": "process",
   "args": [
      "run",
      "--project",
      "${workspaceFolder}/Seminar/NodeCreation/NodeCreation.csproj"
   ],
   "options": {
      "cwd": "./Seminar/NodeCreation"
   },
   "problemMatcher": "$msCompile",
   "group": "none"
}
]
}